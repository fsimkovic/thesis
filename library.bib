isbn = {1858784298},
issn = {10970134},
journal = {Proteins Struct. Funct. Bioinforma.},
keywords = {ComEC,ConPred,RigdenLab,bacterial competence,domain structure,evolutionary covariance,protein modeling},
mendeley-tags = {ConPred,RigdenLab},
