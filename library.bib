@article{Bibby2012,
abstract = {Protein ab initio models predicted from sequence data alone can enable the elucidation of crystal structures by molecular replacement. However, the calculation of such ab initio models is typically computationally expensive. Here, a computational pipeline based on the clustering and truncation of cheaply obtained ab initio models for the preparation of structure ensembles is described. Clustering is used to select models and to quantitatively predict their local accuracy, allowing rational truncation of predicted inaccurate regions. The resulting ensembles, with or without rapidly added side chains, solved 43{\%} of all test cases, with an 80{\%} success rate for all-[alpha] proteins. A program implementing this approach, AMPLE, is included in the CCP4 suite of programs. It only requires the input of a FASTA sequence file and a diffraction data file. It carries out the modelling using locally installed Rosetta, creates search ensembles and automatically performs molecular replacement and model rebuilding.},
author = {Bibby, Jaclyn and Keegan, Ronan M and Mayans, Olga and Winn, Martyn D and Rigden, Daniel J},
doi = {10.1107/S0907444912039194},
isbn = {1399004714006},
