}
@article{Keegan2015,
abstract = {{\{}$\backslash$it AMPLE{\}} clusters and truncates {\{}$\backslash$it ab initio{\}} protein structure predictions, producing search models for molecular replacement. Here, an interesting degree of complementarity is shown between targets solved using the different {\{}$\backslash$it ab initio{\}} modelling programs {\{}$\backslash$it QUARK{\}} and {\{}$\backslash$it ROSETTA{\}}. Search models derived from either program collectively solve almost all of the all-helical targets in the test set. Initial solutions produced by {\{}$\backslash$it Phaser{\}} after only 5min perform surprisingly well, improving the prospects for {\{}$\backslash$it in situ{\}} structure solution by {\{}$\backslash$it AMPLE{\}} during synchrotron visits. Taken together, the results show the potential for {\{}$\backslash$it AMPLE{\}} to run more quickly and successfully solve more targets than previously suspected.},
author = {Keegan, Ronan M and Bibby, Jaclyn and Thomas, Jens M H and Xu, Dong and Zhang, Yang and Mayans, Olga and Winn, Martyn D and Rigden, Daniel J},
