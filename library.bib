issn = {10902716},
journal = {J. Comput. Phys.},
keywords = {Algorithm,ConPred,Contact map,Direct-coupling analysis,Inference,Potts model,Protein structure prediction,Pseudolikelihood},
mendeley-tags = {Algorithm,ConPred},
