author = {Maresso, Anthony W and Wu, Ruiying and Kern, Justin W and Zhang, Rongguang and Janik, Dorota and Missiakas, Dominique M and Duban, Mark Eugene and Joachimiak, Andrzej and Schneewind, Olaf},
