issn = {14712105},
journal = {BMC Bioinformatics},
keywords = {2D prediction,Algorithm,BioXSD,ConPred,Debian package,EVcouplings,EVfold,Fast protein contact prediction,Open-source software,PSICOV,Protein sequence analysis,Protein structure prediction,mfDCA},
language = {en},
mendeley-tags = {Algorithm,ConPred},
month = {mar},
